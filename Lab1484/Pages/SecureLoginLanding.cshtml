@page
@model Lab1484.Pages.SecureLoginLandingModel
@{
}


@{
    if (@ViewData["LoginMessage"] != null)
    {
        <p>@ViewData["LoginMessage"]</p>
        <a asp-page="DBLogin" asp-route-logout="true">Logout of application -></a>
    }

    else
    {
        <p>@ViewData["LoginError"]</p>
        //<a asp-page="/Logout" class="btn btn-primary position-relative">CLick here to log in.</a>
    }

}
